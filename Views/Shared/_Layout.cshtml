                                        <li>@Html.ActionLink("Manage Requests", "Index", "Request")</li>
                                    </ul>
                                </li>
                            }
                        </ul>
                    </div>
                </div>

            </header>

            <div class="main">
                @RenderBody()
            </div>
        </div>
    </div>


</body>
</html>
